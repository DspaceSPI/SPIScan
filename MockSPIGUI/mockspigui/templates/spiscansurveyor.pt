<form class="well">          
<table border="0" align="center">
<TBODY>
	<TR><TD align="center" colspan="4"><h3>Deployment Camera View</h3>
        <TR><TD align="center" colspan="4"><!--<iframe width="200" height="150" src="http://youtu.be/eh_HUIJkRzU" frameborder=1 name="webcamfeed" scrolling="auto" allowfullscreen></iframe>-->
	<TR><TD align="center"><a class="btn" href="#" >Lasers<i class="icon-asterisk"></i></a>
	<TD align="center"><a class="btn" href="#">Start<i class="icon-play"></i></a><TD align="center"><a class="btn" href="#">Stop<i class="icon-stop"></i></a><TD align="center"><a class="btn" href="#">Capture<i class="icon-camera"></i></a>
	<TR><TD align="center" colspan="4"><label class="control-label" for="select01">Scan Resolution</label>
		<div class="controls">
		<select id="scanresolution">
		<option> 75 dpi</option>
		<option>125 dpi</option>
		<option>300 dpi</option>
		<option>600 dpi</option>		
		</div>
	<TR><TD align="center" colspan="4"><a class="btn btn-warning btn-large">Save Scan</a>
	<TR><TD align="center" colspan="2"><a class="btn btn-primary btn-large">Preview</a>
	<TD align="center" colspan="2"><a class="btn btn-primary btn-large">Acquire</a>
	<TR><TD align="center" colspan="2"><a class="btn btn-inverse btn-large">Pump 1</a><TD align="center" colspan="2"><a class="btn btn-inverse btn-large">Pump 2</a>
	<TR><TD align="left" colspan="4">File Prefix<nbsp><nbsp><input type="text" class="input-small">
	<TR><TD align="left" colspan="4"><label class="control-label" for="textarea">Scan Memo</label><textarea class="input-xlarge" id="memo" rows="3"></textarea>
	<TR><TD align="center" colspan="4"><label class="control-label" for="select01">LED Brightness</label>
		<div class="controls">
		<select id="LEDBright">
		<option> Off</option>
		<option> 10%</option>
		<option> 20%</option>
		<option> 30%</option>
		<option> 40%</option>
		<option> 50%</option>
		<option> 60%</option>
		<option> 70%</option>
		<option> 80%</option>
		<option>100%</option>
		</div>
	<TR><TD colspan="2"><label class="checkbox"><input type="checkbox">Autoscan</label>
</TBODY>
