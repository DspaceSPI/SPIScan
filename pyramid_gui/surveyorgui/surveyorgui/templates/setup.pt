<script>
var JSONOBJECTdefaultValues [
{"topmarg":0,"startmarg":0,"endmarg":15,"botmarg":0,"pretopmarg":0,"prestartmarg":0}
];

var endmargdefault;
var emax;
var emin;
endmargdefault = 15;
emax = 200;
emin = 12;

function willRobinson()
{
alert("Broken Page Alert");
}

function setupDefaults() 
{
    document.getElementById('endmargin').value = endmargdefault;
	return;
}

function validateSetup()
{
var endmarg 
endmarg = document.getElementById('endmargin').value;
if (endmarg == "") 
	{
	window.alert("End Margin Can't be Empty");
	document.getElementById('endmargin').value = endmargdefault;
	return;
	}
if (endmarg < (emin))
	{
	window.alert("End Margin Must be 12-200 mm");
	document.getElementById('endmargin').value = endmargdefault;
	return;
	}
if (endmarg > (emax))
	{
	window.alert("End Margin Must be 12-200 mm");
	document.getElementById('endmargin').value = endmargdefault;
	return;
	}
if (isNaN(endmarg))
	{
	window.alert("End Margin Must be a Number");
	document.getElementById('endmargin').value = endmargdefault;
	return;
	}
}
</script>
